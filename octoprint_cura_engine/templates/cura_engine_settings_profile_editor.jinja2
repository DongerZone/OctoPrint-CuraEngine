<div id="settings_plugin_cura_engine_profile_edit" class="modal hide fade">
    <div class="modal-header">
        <a href="#" class="close" data-dismiss="modal" aria-hidden="true">&times;</a>
        <h3 class="modal-title"></h3>
    </div>
    <div class="modal-body">
        <div class="full-sized-box">
            <form name="profileEditorForm" class="form-horizontal">
                <div data-bind="foreach: profileEditorStruct">
                    <span data-bind="createSettingsFields: $data"></span>
                </div>
             </form>
        </div>
    </div>
    <div class="modal-footer">
        <button class="btn btn-primary" data-bind="click: onProfileEditorSave" aria-hidden="true">{{ _('Save') }}</button>
        <button class="btn" data-dismiss="modal" data-bind="click: onProfileEditorCancel" aria-hidden="true">{{ _('Cancel') }}</button>
    </div>
</div>